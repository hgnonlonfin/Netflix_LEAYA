#Importation des librairies et des bases donn√©es
import pandas as pd
dfLinks=pd.read_csv("links.csv") 
dfMovies=pd.read_csv("movies.csv")
dfRatings=pd.read_csv("ratings.csv")
dfTags=pd.read_csv("tags.csv")

#Fusion des bases dfMovies et dfLinks
dfMoviesLinks=dfMovies.merge(dfLinks, on="movieId", how="inner")

#Fusion des informations sur les users
dfRatingsTags=dfRatings.merge(dfTags, on=["userId","movieId"], how='left')

#Fusion des informations sur les movies et les users
df=dfMoviesLinks.merge(dfRatingsTags, on="movieId", how="inner")
